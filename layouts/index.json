{{- $pages := slice -}}
{{- range .Site.RegularPages -}}
  {{- $links := slice -}}
  {{- $content := .RawContent -}}
  {{- /* Simple regex to find wikilinks [[Link]] */ -}}
  {{- $matches := findRE `\[\[([^\]]+)\]\]` $content -}}
  {{- range $matches -}}
    {{- $linkTarget := replace . "[[" "" -}}
    {{- $linkTarget = replace $linkTarget "]]" "" -}}
    {{- $links = $links | append $linkTarget -}}
  {{- end -}}
  
  {{- $pageData := dict "id" .RelPermalink "title" .Title "status" (.Params.status | default "seeding") "type" .Params.type "links" $links -}}
  {{- $pages = $pages | append $pageData -}}
{{- end -}}
{{- $pages | jsonify -}}
